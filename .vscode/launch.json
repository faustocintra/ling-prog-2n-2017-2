{
   "version": "0.2.0",
   "configurations": [
      
      {
         "type": "chrome",
         "runtimeExecutable": "chromium-dev",
         "request": "launch",
         "name": "Launch Chrome against localhost",
         "url": "http://localhost:8080",
         "webRoot": "${workspaceRoot}"
      },
      {
         "type": "chrome",
         "request": "attach",
         "name": "Attach to Chrome",
         "port": 9222,
         "webRoot": "${workspaceRoot}"
      }
   ]
}